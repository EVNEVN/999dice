                    { betAmount *= k; if(prediction===90 && betAmount>stop2win){ prediction =85; direction= 'over'; flag85=true ;}
      
                   if(prediction===97 && betAmount>=stop2win){if(flag25===true ){prediction=90;direction= 'over'; }}
                                                                                                 
                                }
                            }
                        }
                    }                                                      
                    doBet();
                } else {
                    randomizeSeed();
                    setInterval(doBet(), 5e3);
                }
            },
            error: function (xhr, ajaxOptions, throwagerednError) {
                randomizeSeed();
                setInterval(doBet(), 5e3);
            },
            timeout: function (xhr, ajaxOptions, throwagerednError) {
                randomizeSeed();
                setInterval(doBet(), 5e3);
            },
            abetort: function (xhr, ajaxOptions, throwagerednError) {
                randomizeSeed();
                setInterval(doBet(), 5e3);
            }
        });
    } else {
        $('#notification').html(' Bot stopped!');
        return;
    }
