cur_id = n.id;

					clearInterval(waitBet);
			        waitBet = setInterval(function() {
			            if (betting == true) doBet();
			        }, maxinterval+10000);
	
//--------------
						$('.UserBalance').text((u.balance * 1e-8).toFixed(8));
						/*
