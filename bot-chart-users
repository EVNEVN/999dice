return;
}
function big()
{ plus=plus+1;
 if (plus===1){BigBet=0.0005;}
 if (plus===2){BigBet=0.001;}
 if (plus===3){BigBet=0.002;} 
 if (plus===4){BigBet=0.005;}
 if (plus===5){BigBet=0.01;}
 if (plus===6){BigBet=0.05;}
 if (plus===7){BigBet=0.1;}
 if (plus===8){BigBet=0.16;}
 if(plus===9){plus=0;}
 return;         
}

