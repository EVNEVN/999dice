		$('#activemode').html('(Optimal,2)');
		$('#activemode').css('color', 'yellow');
	}
	if ($('#radio3').is(':checked')) {
		$('#activemode').html('(Risk)');
		$('#activemode').css('color', 'yellow');
	}
