    authenETUserr = JSON.parse(datauserr);
    for (iii = 0; iii < authenETUserr.length; iii++)
        if (authenETUserr[iii] == currentETUserr) checkETUserr = true;
    if (checkETUserr == true) {
    		
    	}else{
