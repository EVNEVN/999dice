	if (resetModeBetsRound % resetModeBets === 0) {
		//rd_strategy = Math.floor((Math.random() * (7 - 1 + 1)) + 1);
		randomStrategy();
	}
	if (resetModeWinsRound % resetModeWins === 0) {
		//rd_strategy = Math.floor((Math.random() * (7 - 1 + 1)) + 1);
		randomStrategy();
	}
	if (resetModeLossesRound % resetModeLosses === 0) {
		//rd_strategy = Math.floor((Math.random() * (7 - 1 + 1)) + 1);
