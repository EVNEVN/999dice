		$('#activemode').html('(Series)');
		$('#activemode').css('color', 'Violet');
	}
	if ($('#radio18').is(':checked')) {
		$('#activemode').html('(Optimal-S)');
		$('#activemode').css('color', 'Lime');
	}
	if ($('#radio19').is(':checked')) {
		$('#activemode').html('(Fibonacci)');
		$('#activemode').css('color', 'crimson');
