                        $('#notification').html(' You can take a new lease term.');

                        return; 
                    } else {
                        if (overBalance != 0 && balance >= overBalance) {
                            stop();
                            $('#notification').html('OverBalance!  Target is Alright ');
                            return;   

                       } else if (underBalance != 0 && balance <= underBalance) {
                            stop();
