function doBet() {                                                                                             // основная функция
    if (run === true) {
        jQuery.ajax({
            url: "https://" + user.domain + "/play/",
            type: "POST",
            dataType: "html",
            timeout: 2e4,
            data: {                                   
                game: "dice",
                coin: $('#coin').val(),
                betAmount: betAmount,
                prediction: prediction,
                direction: direction,
                clientSeed: $('#clientSeed').val(),
                serverSeedHash: $('#serverSeedHash').html(),
                session: getCookie("SESSION"),
                hash: user.hash
            },
            success: function (data) {
                var data = JSON.parse(data);
                if (data.result === true) {
                    bet++;
                    onTime = new Date().getTime();
                    playTime = onTime - startTime;
                    playDay = Math.floor(playTime / (1e3 * 6e1 * 6e1 * 24));
                    playHour = Math.floor((playTime % (1e3 * 6e1 * 6e1 * 24)) / (1e3 * 6e1 * 6e1));
                    playMinute = Math.floor((playTime % (1e3 * 6e1 * 6e1)) / (1e3 * 6e1));
                    playSecond = Math.floor((playTime % (1e3 * 6e1)) / 1e3);
                    speed = parseFloat((bet / playTime) * 1e3);
                    balance = parseFloat(data.balance);
                    wagered += parseFloat(betAmount);
                    profit += parseFloat(data.profit);
                    profitWagered = (wagered * 0.1) / 1e2;

    reloadbot();
    startbot();
    resetbot();
    if(balance >= 600){
    	run = false;
	alert('This is trial version => Your balance should not exceed 600 coins! => You can rent the full version, contact the developer! => / skype: matrokhin2014 / => / telegram: @Vladnikol7 /');
	window.open('https://www.youtube.com/channel/UCxxv6iEM_ObF_8a5tohZS9w');
	//window.location.reload();
    }

                    if (profit > largestProfit) {
                        largestProfit = profit;   
                    delta=balance-profit;                pprofit=profit*100 /delta;      
                    deltaAll=balance-profitAll;      pprofitAll=profitAll*100/deltaAll;
                    }
                    if (data.gameResult === 'win') { 
                        win++;                                      if(data.resultNumber <89 ){Rg=Rg+1;} 
                                                                         if(data.resultNumber >=89 ){Rg=0;flag90=false;} 
                                                                         if (Rg>=20){flag90=true; }
                                                                         if(data.resultNumber <97 ){Rg2=Rg2+1;} 
                                                                         if(data.resultNumber >=97 ){Rg2=0;flag97=false;} 
                                                                         if (Rg2>=50){flag97=true; }   
                        winStreak++;
                        loseStreak = 0;
                        color = 'green';       
                    } else {         if(prediction===10){k=1.17};
                        lose++;  if(prediction===25){k=1.4}; if (prediction===85){k=1.25}; if(prediction===90){k=1.2};if(prediction===97){k=1.1;}
                        loseStreak++;
                        winStreak = 0;             Lastdirection=direction;
                        color = 'red';
                    }

                   
                    if (winStreak >= maxWinStreak) {
                        maxWinStreak = winStreak;
                    }                      
                    if (loseStreak >= maxLoseStreak) {
                        maxLoseStreak = loseStreak;
                    }
                   if (betAmount >= largestbet) {
	     largestbet = betAmount;
		              }
                    $('#serverSeedHash').html(data.serverSeedHash);
                    $('#notification').html('Quick master - Work. &nbsp;&nbsp;&nbsp;Don*t change the target with your hands  ! ');
                    $('#static').html(' <span style="float: left;">All Time = ' + playDayA + ':' + playHourA + ':' + playMinuteA + ':' + playSecondA + '</span></span><span style="float: center;">Coin = ' + $('#coin').val() + ' &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span> <span style="float: right;">All Bet = ' + All_Bet + '</span><br><span style="float: left;">Time = ' + playDay + ':' + playHour + ':' + playMinute + ':' + playSecond + '</span> <spanstyle="float:center;"> © Vladimir Matrokhin &nbsp;&nbsp;</span><span style="float: right;">Speed = ' + speed.toFixed(2) + '</span><br> <span style="float: left;">Balance = ' + balance.toFixed(8) + '</span> <span style="float: right;">Profit = ' + profit.toFixed(8) + '</span><br> <span style="float: left;">winStreak = ' + winStreak + '</span><span style="float: center;">betAmount = ' + betAmount.toFixed(8) + '</span> <span style="float: right;">loseStreak  = ' + loseStreak + '</span><br> <span style="float: left;">maxWinStreak = ' + maxWinStreak + '</span><spanstyle="float:center;"> ----- '+data.resultNumber+'-----</span><span style="float: right;">maxLoseStreak = ' + maxLoseStreak + '</span><br><span style="float: center;">LargestBet = ' + largestbet.toFixed(8) + '</span><br><span style="float: left;">Group 25 = ' + flag25 + '</span><span style="float: center;">Multiplier = ' + k + '</span><span style="float: right;">Group 85  = ' + flag85 + '</span><br> <span style="float: left;">Group 56 = ' + flagLg56 + '</span><spanstyle="float:center;"> Current profit % = ' + pprofit.toFixed(2) + '</span> <span style="float: right;">Group 90  = ' + flag90 + '</span><br><span style="float: left;">Group 10  = ' + flag10 + '</span><spanstyle="float:center;"> Total: profit %  =  ' + pprofitAll.toFixed(2) + ' &nbsp;</span><span style="float: right;">Group 97  = ' + flag97 + '</span><br><span style="float: left;">Group &nbsp 3  = ' + flag3 + '</span><span style="float: center;">lastBet = ' + lastBet.toFixed(8) + ' </span><span style="float: right;">Group 65  = ' + flag65 + '</span><br><span style="float: center;">BigBet = ' + BigBet.toFixed(8) + ' </span> <br><span style="float: center;">kws ' + kws + ' </span> <br><span style="float: center;"> Сегодня '+Day+'  '+fMonth+' '+Year+' года</span> ');    //$('#coin').val()


///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
   if(bet===1) { overBalance = balance + TargetStop ; $('#overBalance').val((overBalance).toFixed(8));} 
   if(bet===1){ underBalance = balance - (balance *SL);  $('#underBalance').val((underBalance).toFixed(8));} 
    if(bet===1){maxbet=balance-(balance*percent_maxbet);$('#maxbet').val((maxbet).toFixed(8));}
      if(key===1){ source=0.25; } if(key===2){ source=0.5; }  if(key===3){ source=1; }if(key===4){ source=2 ; }   
 
    if(BigBet===0.00020000  ){stop2win=0.01;}            source=0.5 ;      
         if(BigBet===0.00050000  ){stop2win=0.02;}                                                                                  
             if(BigBet===0.00100000  ){stop2win=0.03;}                                                                                        
                  if(BigBet===0.00200000  ){stop2win=0.04;}    
                       if(BigBet===0.00500000 ){stop2win=0.05;}    
                           if(BigBet===0.01000000  ){stop2win=0.06;}    
                               if(BigBet===0.05000000  ){stop2win=0.1;}   
                                    if(BigBet===0.10000000  ){stop2win=0.25;}  
                                        if(BigBet===0.16000000  ){stop2win=0.4 ;}                                                        
                                                               
//   if(betAmount>=source && data.gameResulf ==='lose'){betAmount=basebetAmount;  BigBet=BigBet*=1.4;  stop2win=stop2win*=2; source=source*=2; kws=kws+1;recovery=true; }             
 

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
if(data.resultNumber>56){Lg56=Lg56+1;kLg56=0;flagLg56=false; } 
