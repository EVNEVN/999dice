                        break;
						/****/
                    default:
                        pipeAuto.events.betError && pipeAuto.events.betError(n.msg)
                }
            }).fail(function() {
                --pipeAuto.server.overlappingBetCount;
                pipeAuto.server.isBetOutstanding = !1
            })))
        },
