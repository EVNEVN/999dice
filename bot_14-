                        break;
						/*****/
                    case 2:
                        pipeAuto.events.betChanceExceedsMaximum && pipeAuto.events.betChanceExceedsMaximum(n.msg);
                        break;
                    case 3:
                        pipeAuto.events.betChanceLessThanMinimum && pipeAuto.events.betChanceLessThanMinimum(n.msg);
                        break;
