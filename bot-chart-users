    coin = $('#coin').val();
    if (coin == "rep" || coin == "bnt" || coin == "bnb" || coin == "btc" || coin == "bch" || coin == "bcd" || coin == "btg" || coin == "bsv" || coin == "link" || coin == "clam" || coin == "mco" || coin == "dash" || coin == "dcr" || coin == "eos" || coin == "eth" || coin == "etc" || coin == "gas" || coin == "zen" || coin == "hc" || coin == "kmd" || coin == "lsk" || coin == "ltc" || coin == "mona" || coin == "xmr" || coin == "nmc" || coin == "nano" || coin == "nebl" || coin == "neo" || coin == "nvc" || coin == "omg" || coin == "ont" || coin == "part" || coin == "ppt" || coin == "qtum" || coin == "sbd" || coin == "strat" || coin == "usdt" || coin == "tusd" || coin == "waves" || coin == "zec" || coin == "xzc") {
    	run = false;
	alert('This is trial version! => You can not use coins: Augur, Bancor, Binance Coin, Bitcoin, Bitcoin Cash, Bitcoin Diamond, Bitcoin Gold, Bitcoin SV, Chainlink, CLAMs, Crypto.com, DASH, Decred, EOS, Ethereum, Ethereum Classic, GAS, Horizen, HyperCash, Komodo, Lisk, Litecoin, MonaCoin, Monero, Namecoin, Nano, Neblio, NEO, Novacoin, OmiseGO, Ontology, Particl, Populous, Qtum, Steem Dollars, STRATIS, Tether, TrueUSD, WAVES, Zcash, Zcoin => You can rent the full version, contact the developer! => / skype: matrokhin2014 / => / telegram: @Vladnikol7 /');
